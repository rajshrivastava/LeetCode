class Solution(object):
    def generateParenthesis(self, n):
        """
        :type n: int
        :rtype: List[str]
        """
        def paren(o, c, beg, end):
            if o>0:
                paren(o-1, c, beg+'(', end)

            if c>=o:
                paren(o, c-1, beg+')', end)
                
            if o==0 and c==0:
                strList.append(beg+end)
        strList = []
        o = n-1
        c = n-1
        paren(o, c, '(',')')
        return strList
        
