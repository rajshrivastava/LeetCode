class Solution:
    def reverseWords(self, s: str) -> str:
        words = []
        n = len(s)
        i=n-1
        end = n-1
        while(i>=0):
            while(i>=0 and s[i]==' '):
                i-=1
            end = i
            while(i>=0 and s[i]!=' '):
                i-=1
            temp = s[i+1:end+1] 
            if temp:
                words.append(temp)
                    
        #return words
        return ' '.join(words)
