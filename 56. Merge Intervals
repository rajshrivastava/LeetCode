class Solution:
    def merge(self, intervals: List[List[int]]) -> List[List[int]]:
        n = len(intervals)
        if n == 0:
            return []
        intervals.sort(key = lambda x:x[0])
        merged_intervals = [intervals[0]]
        top = 0
        for i in range(1, n):
            if intervals[i][0] <= merged_intervals[top][1]:
                merged_intervals[top][1] = max(merged_intervals[top][1], intervals[i][1])
            else:
                merged_intervals.append(intervals[i])
                top+=1
        return merged_intervals
